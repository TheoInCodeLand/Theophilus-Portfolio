            </li>

            <li>
              <button class="card tag-btn">
                <!-- <img src="/img/tag3.png" width="32" height="32" loading="lazy" alt="Lifestyle"> -->

                <p class="btn-text">Startup Solutions</p>
              </button>
            </li>

            <li>
              <button class="card tag-btn">
                <!-- <img src="/img/tag4.png" width="32" height="32" loading="lazy" alt="Fashion"> -->

                <p class="btn-text">Chatbot Training</p>
              </button>
            </li>

            <li>
              <button class="card tag-btn">
                <!-- <img src="/img/tag5.png" width="32" height="32" loading="lazy" alt="Food"> -->

                <p class="btn-text">Problem Solving</p>
              </button>
            </li>

            <li>
              <button class="card tag-btn">
                <!-- <img src="/img/tag6.png" width="32" height="32" loading="lazy" alt="Space"> -->

                <p class="btn-text">Creative Solutions</p>
              </button>
            </li>

            

          </ul>

        </div>
      </section>

      

      <!-- #Blog posts -->
      <section class="section recent-post" id="recent" aria-labelledby="recent-label">
        <div class="container">

          <div class="post-main">

            <h2 class="headline headline-2 section-title">
              <span class="span">Latest Articles</span>
            </h2>

            <p class="section-text">
              Insights from my coding journey...
            </p>

            <ul class="grid-list">

              <% if (blogs.length === 0) { %>
                 <p>No articles posted yet.</p>
              <% } else { %>
                
                <% blogs.forEach(blog => { %>
                <li>
                  <div class="recent-post-card">

                    <figure class="card-banner img-holder" style="--width: 271; --height: 258;">
                      <img src="<%= blog.image_url || '/img/placeholder-blog.jpg' %>" width="271" height="258" loading="lazy"
                        alt="<%= blog.title %>" class="img-cover">
                    </figure>

                    <div class="card-content">

                      <a href="<%= blog.external_link %>" target="_blank" class="card-badge"><%= blog.category %></a>

                      <h3 class="headline headline-3 card-title">
                        <a href="<%= blog.external_link %>" target="_blank" class="link hover-2">
                          <%= blog.title %>
                        </a>
                      </h3>

                      <div class="card-wrapper">
                        <div class="wrapper">
                          <ion-icon name="time-outline" aria-hidden="true"></ion-icon>
                          <span class="span"><%= blog.read_time %></span>
                        </div>
                        
                        <a href="<%= blog.external_link %>" target="_blank" class="span hover-2">
                            Read <i class="fa-solid fa-arrow-up-right-from-square" style="font-size: 0.8rem;"></i>
                        </a>
                      </div>

                    </div>

                  </div>
                </li>
                <% }) %>

              <% } %>

            </ul>

          </div>

          <div class="post-aside grid-list">

            <div class="card aside-card insta-card">

              <a href="#" class="logo">
                <i class="fa-solid fa-arrows-turn-to-dots"></i> .codeNexus
              </a>

              <p class="card-text">
                My Tech Stack & Tools
              </p>

              <ul class="insta-list" style="grid-template-columns: repeat(3, 1fr); gap: 10px;">
                <% if (skills) { %>
                    <% skills.slice(0, 9).forEach(skill => { %>
                        <li style="text-align: center; background: #1e293b; padding: 10px; border-radius: 8px;">
                            <i class="<%= skill.icon_class %>" style="font-size: 24px; color: #fff;"></i>
                        </li>
                    <% }) %>
                <% } %>
              </ul>

            </div>

          </div>

        </div>
      </section>

