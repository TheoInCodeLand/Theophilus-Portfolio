      </section>

      <!-- #Technologies & Skills -->
      <section class="tags" aria-labelledby="tag-label">
        <div class="container">

            
          <h2 class="headline headline-2 section-title" id="tag-label">
            <span class="span">Technologies & Skills</span>
          </h2>

          <p class="section-text">
            Explore my expertise in modern web and software development.
          </p>

          <ul class="grid-list">
            <% if (skills && skills.length > 0) { %>
              
              <% skills.forEach(skill => { %>
                <li>
                  <button class="card tag-btn">
                    <i class="<%= skill.icon_class %>"></i>

                    <p class="btn-text"><%= skill.name %></p>
                  </button>
                </li>
              <% }) %>

            <% } else { %>
              <p>No skills listed yet.</p>
            <% } %>

          </ul>

        </div>
      </section>

      <!-- #Featured Projects -->
      <section class="section feature" aria-label="feature" id="featured">
        <div class="container">

          <h2 class="headline headline-2 section-title">
            <span class="span">Featured Projects</span>
          </h2>

          <p class="section-text">
            Explore some of my best and most impactful projects that showcase my skills and expertise.
          </p>

          <ul class="feature-list">

            <% if (projects.length === 0) { %>
              <p>No projects added yet. Check back soon!</p>
            <% } else { %>
              
              <% projects.forEach(project => { %>
              <li>
                <div class="card feature-card">

                  <figure class="card-banner img-holder" style="--width: 1602; --height: 903;">
                    
                    <% if (project.video_url && project.video_url.endsWith('.mp4')) { %>
                      <video src="<%= project.video_url %>" width="1602" height="903" loading="lazy" preload="auto"
                        playsinline loop muted class="img-cover"
                        autoplay controls></video>
                    <% } else if (project.video_url) { %>
                      <img src="<%= project.video_url %>" width="1602" height="903" loading="lazy"
                        alt="<%= project.title %>" class="img-cover">
                    <% } else { %>
                      <img src="/img/placeholder.png" width="1602" height="903" loading="lazy"
                        alt="Project Placeholder" class="img-cover">
                    <% } %>

                  </figure>

                  <div class="card-content">

                    <div class="card-wrapper">
                      <div class="card-tag">
                        <% if (project.tech_stacks) { %>
                          <% project.tech_stacks.forEach(stack => { %>
                            <a href="#" class="span hover-2">#<%= stack %></a>
                          <% }) %>
                        <% } %>
                      </div>

                      <div class="wrapper">
                        <ion-icon name="time-outline" aria-hidden="true"></ion-icon>
                        <span class="span"><%= project.duration_months %> Months Dev</span>
                      </div>
                    </div>

                    <h3 class="headline headline-3">
                      <a href="<%= project.demo_link || '#' %>" class="card-title hover-2">
                        <%= project.title %>
                      </a>
                    </h3>

                    <div class="card-wrapper">

                      <div class="profile-card">
                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTJrPYCaKX2rBLNV10BOEmf_lrzbxbudvIpaA&s" width="48" height="48" loading="lazy" alt="Author"
                          class="profile-banner">

                        <div>
                          <p class="card-subtitle">Completed: <%= new Date(project.date_completed).getFullYear() %></p>
                        </div>
                      </div>

                      <% if (project.github_link) { %>
                        <a href="<%= project.github_link %>" class="card-btn" target="_blank"><i class="fa-brands fa-github"></i> Github</a>
                      <% } %>
                      
                      <% if (project.demo_link) { %>
                        <a href="<%= project.demo_link %>" class="card-btn" target="_blank"><i class="fa-solid fa-globe"></i> Demo</a>
                      <% } %>

                    </div>

                  </div>

                </div>
              </li>
              <% }) %>

            <% } %>

          </ul>

          <a href="#" class="btn btn-secondary">
            <span class="span">View More Projects</span>
            <ion-icon name="arrow-forward" aria-hidden="true"></ion-icon>
          </a>

        </div>

        <img src="/img/shadow-3.svg" width="500" height="1500" loading="lazy" alt="" class="feature-bg">

      </section>

      <!-- #Interests & Passions -->

      <section class="tags" aria-labelledby="tag-label">
